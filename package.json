{
  "name": "aiders-api",
  "version": "0.1.0",
  "description": "an api for emergency aiders app",
  "main": "app",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf dist/ && babel ./src --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "babel-node": "babel-node --presets=env --plugins=transform-class-properties,transform-es2015-arrow-functions",
    "start": "nodemon --exec yarn run babel-node -- ./src/app"
  },
  "author": "Tobi Taiwo (rebirthtobi)",
  "license": "MIT",
  "dependencies": {
    "express": "^4.16.3",
    "express-handlebars": "^3.0.0",
    "idb": "^2.1.3",
    "rimraf": "^2.6.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "nodemon": "^1.17.5"
  },
  "engines": {
    "node": "9.10.0"
  }
}
